<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <move element="form.subscribe" destination="main.content" after="home_instagram"/>
        <move element="columns" destination="main.content" after="home_tiles"/>
        <move element="page.messages" destination="main.content" before="-"/>
         <referenceBlock name="form.subscribe" display="true"/>
        <referenceContainer name="main.content">
            <block class="Magento\Framework\View\Element\Template" name="home_banner" before="-" template="Magento_Cms::home_banner.phtml" />
            <block class="Magento\Framework\View\Element\Template" name="home_tiles" after="home_banner" template="Magento_Cms::home_tiles.phtml" />
            <block class="Magento\Framework\View\Element\Template" name="home_product" after="home_superfood" template="Magento_Cms::home_product.phtml" />
            <block class="Magento\Framework\View\Element\Template" name="home_instagram" after="home_product" template="Magento_Cms::home_instagram.phtml" />
            <block class="Magento\Framework\View\Element\Template" name="home_subcribe" after="home_instagram" template="Magento_Cms::home_subcribe.phtml" />
        </referenceContainer>
        <referenceContainer name="main">
            <block class="Magento\Framework\View\Element\Template" name="home_superfood" before="-" template="Magento_Cms::home_superfood.phtml" />
        </referenceContainer>
    </body>
</page>
